{
  "manifest_version": 3,
  "name": "Eagle Plugin",
  "description": "A simple Chrome extension with Vue 3, Vite, and Element Plus",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "assets/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
